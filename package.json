{
  "name": "api-model-tree",
  "version": "0.8.0",
  "description": "Convert JsonSchema based document into structured and simplified data tree",
  "module": "dist/esm/index.js",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist",
    "browser"
  ],
  "browser": {
    "./dist/cjs/index.js": "./browser/api-model-tree.js"
  },
  "homepage": "https://github.com/udamir/api-model-tree",
  "bugs": "https://github.com/udamir/api-model-tree",
  "author": "Damir Yusipov",
  "repository": {
    "type": "git",
    "url": "https://github.com/udamir/api-model-tree"
  },
  "license": "MIT",
  "keywords": [
    "json",
    "schema",
    "json-schema",
    "jsonschema",
    "openapi",
    "swagger",
    "tree",
    "simplify"
  ],
  "scripts": {
    "build": "tsc && tsc --module commonjs --outDir dist/cjs",
    "test": "jest --verbose",
    "prepublish": "rm -r dist || true && yarn build && yarn build:web",
    "test:coverage": "jest --verbose --coverage",
    "build:web": "vite build"
  },
  "peerDependencies": {},
  "dependencies": {
    "@types/json-schema": "^7.0.12",
    "allof-merge": "0.5.1",
    "gqlapi": "^0.5.1",
    "json-crawl": "^0.2.6"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/js-yaml": "^4.0.5",
    "@types/treeify": "^1.0.0",
    "benchmark": "^2.1.4",
    "graphql": "^16.8.0",
    "jest": "^29.5.0",
    "js-yaml": "^4.1.0",
    "treeify": "^1.1.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.2",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^3.0.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(ts?|tsx?|js?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "collectCoverage": true
  }
}
