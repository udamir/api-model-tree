{
  "name": "api-model-tree",
  "version": "0.10.4",
  "description": "Convert JsonSchema based document into structured and simplified data tree",
  "module": "dist/index.mjs",
  "main": "dist/index.cjs",
  "browser": "dist/index.iife.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "homepage": "https://github.com/udamir/api-model-tree",
  "bugs": "https://github.com/udamir/api-model-tree",
  "author": "Damir Yusipov",
  "repository": {
    "type": "git",
    "url": "https://github.com/udamir/api-model-tree"
  },
  "license": "MIT",
  "keywords": [
    "json",
    "schema",
    "json-schema",
    "jsonschema",
    "openapi",
    "swagger",
    "tree",
    "simplify"
  ],
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "rollup -c",
    "test": "jest --verbose",
    "test:coverage": "jest --verbose --coverage"
  },
  "dependencies": {
    "@types/json-schema": "^7.0.12",
    "allof-merge": "^0.6.1",
    "gqlapi": "^0.5.1",
    "json-crawl": "^0.3.1",
    "openapi-types": "^12.1.3"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.2",
    "@types/js-yaml": "^4.0.5",
    "@types/treeify": "^1.0.0",
    "api-smart-diff": "^0.7.2",
    "graphql": "^16.8.0",
    "jest": "^29.5.0",
    "js-yaml": "^4.1.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.6",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-progress": "^1.1.2",
    "treeify": "^1.1.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.2",
    "typescript": "^5.1.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(ts?|tsx?|js?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "collectCoverage": true
  }
}
